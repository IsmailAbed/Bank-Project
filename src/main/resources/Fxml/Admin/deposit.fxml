<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="680.0" prefWidth="750.0" stylesheets="@../../Styles/Deposit.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.abedbank.Controllers.Admin.depositController">
    <VBox layoutX="9.0" layoutY="128.0" prefHeight="515.0" prefWidth="730.0" AnchorPane.leftAnchor="9.0" AnchorPane.topAnchor="128.0">
        <HBox alignment="CENTER" prefHeight="60.0" prefWidth="200.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Search By Payee Address" />
            <TextField fx:id="pAddress_fld" styleClass="search_box" />
            <Button fx:id="search_btn" mnemonicParsing="false" text="Search">
                <graphic>
                    <FontAwesomeIconView fill="#fffbfb" glyphName="SEARCH" size="17" />
                </graphic>
            </Button>
        </HBox>
        <ListView prefHeight="200.0" prefWidth="200.0" styleClass="search_box" />
        <VBox alignment="TOP_CENTER" prefHeight="259.0" prefWidth="730.0" styleClass="deposit_box">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Deposite Section" />
            <Text fx:id="result_listview" strokeType="OUTSIDE" strokeWidth="0.0" text="Amount:" />
            <TextField fx:id="amount_fld" maxWidth="-Infinity" prefHeight="25.0" prefWidth="161.0" styleClass="search_box" />
            <Button fx:id="deposit_btn" mnemonicParsing="false" text="Deposit">
                <graphic>
                    <FontAwesomeIconView fill="WHITE" glyphName="MONEY" size="20" />
                </graphic>
            </Button>
        </VBox>
    </VBox>
</AnchorPane>
